<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="150"
        x:Class="_4080capstone.Views.UsernameSetWindow"
        Title="Set Username"
		Padding="30"
		CanResize ="False"
		Width="300"
		Height="150">
	<StackPanel VerticalAlignment="Center" >
		<Label>Enter username</Label>
		<TextBox Name="UsernameInput" Margin="0 5 0 10"></TextBox>
		<Button HorizontalAlignment="Right" Name="SetUsernameBtn" Classes="purpleBtn" Click="SetUsernameBtn_Click">Set Username</Button>
		<!-- cancel button? -->
	</StackPanel>
</Window>
